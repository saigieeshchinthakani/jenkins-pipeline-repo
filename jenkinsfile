pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'compiling source code...'
            }
        }
        stage('Test') {
            steps {
                echo 'testing source code...'
            }
        }
        stage('Package') {
            steps {
                echo 'creating package...'
            }
        }
	stage('deploy') {
		steps {
			echo 'deploying the  package......'
		}
	}
    }
}
